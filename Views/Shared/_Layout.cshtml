    <script>

        function sendSelectedValue() {
            var selectedValue = document.getElementById("User_Type").value;

            var formData = {
                User_Type: selectedValue
            };

            $.ajax({
                type: 'POST',
                url: 'Home/SaveSelectedValue',
                data: formData,
                success: function (data) {

                },
                error: function () {
                    $('.preloader').hide();
                    // Gestisci gli errori qui, ad esempio mostrando un messaggio di errore all'utente
                }
            });
        }

        function ShowModal() {

            $('#centermodal').modal('show');

        }



    </script>

